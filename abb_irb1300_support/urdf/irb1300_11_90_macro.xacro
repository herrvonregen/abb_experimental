<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro"/>

    <xacro:macro name="abb_irb1300_11_90" params="prefix">
        <!-- link list -->
        <link name="${prefix}base_link">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/base_link.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/base_link.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_1">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_1.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_1.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_2">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_2.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_2.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_3">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_3.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_3.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_4">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_4.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_4.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_5">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_5.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_5.dae" />
                </geometry>

            </visual>
        </link>

        <link name="${prefix}link_6">
            <collision name="collision">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/collision/link_6.stl"/>
                </geometry>
                <xacro:material_abb_yellow/>
            </collision>
            <visual name="visual">
                <geometry>
                    <mesh filename="package://abb_irb1300_support/meshes/irb1300_11_90/visual/link_6.dae" />
                </geometry>
            </visual>
        </link>
        <!-- end of link list -->

        <!-- joint list -->
        <joint type="revolute" name="${prefix}joint_1">
            <origin xyz="0 0 0.254" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}link_1"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-180)}" upper="${radians(180)}" velocity="${radians(243)}"/>
            <dynamics damping="50.0" friction="1.0" />
        </joint>
        <joint type="revolute" name="${prefix}joint_2">
            <origin xyz="0.050 0 0.290" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <parent link="${prefix}link_1"/>
            <child link="${prefix}link_2"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-100)}" upper="${radians(130)}" velocity="${radians(228)}"/>
            <dynamics damping="50.0" friction="1.0" />
        </joint>
        <joint type="revolute" name="${prefix}joint_3">
            <origin xyz="0 0 0.425" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <parent link="${prefix}link_2"/>
            <child link="${prefix}link_3"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-210)}" upper="${radians(65)}" velocity="${radians(300)}"/>
            <dynamics damping="10.0" friction="1.0"/>
        </joint>
        <joint type="revolute" name="${prefix}joint_4">
            <origin xyz="0.1295 0 0.04" rpy="0 0 0"/>
            <axis xyz="1 0 0"/>
            <parent link="${prefix}link_3"/>
            <child link="${prefix}link_4"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-230)}" upper="${radians(230)}" velocity="${radians(500)}"/>
            <dynamics damping="5.0" friction="1.0"/>
        </joint>
        <joint type="revolute" name="${prefix}joint_5">
            <origin xyz="0.295574 0 0" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <parent link="${prefix}link_4"/>
            <child link="${prefix}link_5"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-130)}" upper="${radians(130)}" velocity="${radians(420)}"/>
            <dynamics damping="2.0" friction="1.0"/>
        </joint>
        <joint type="revolute" name="${prefix}joint_6">
            <origin xyz="0.084 0 0" rpy="0 0 0"/>
            <axis xyz="1 0 0"/>
            <parent link="${prefix}link_5"/>
            <child link="${prefix}link_6"/>
            <!-- See note 2 in package.xml about effort limits and dynamics values -->
            <limit effort="1000" lower="${radians(-242)}" upper="${radians(242)}" velocity="${radians(720)}"/>
            <dynamics damping="1.0" friction="1.0"/>
        </joint>
        <!-- end of joint list -->

        <!-- ROS-Industrial 'base' frame: base_link to ABB World Coordinates transform -->
        <link name="${prefix}base" />
        <joint name="${prefix}base_link-base" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <parent link="${prefix}base_link"/>
            <child link="${prefix}base"/>
        </joint>

        <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
        <link name="${prefix}flange"/>
        <joint type="fixed" name="${prefix}joint_6-flange">
            <origin xyz="0.006 0 0" rpy="0 0 0"/>
            <parent link="${prefix}link_6"/>
            <child link="${prefix}flange"/>
        </joint>

        <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
        <link name="${prefix}tool0" />
        <joint name="${prefix}link_6-tool0" type="fixed">
            <origin xyz="0 0 0" rpy="0 ${radians(90)} 0" />
            <parent link="${prefix}flange" />
            <child link="${prefix}tool0" />
        </joint>
    </xacro:macro>
</robot>